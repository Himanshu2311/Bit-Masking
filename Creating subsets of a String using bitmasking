#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long ull;
#define tez ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define f(i,a,b) for(ll i=a;i<b;i++)
#define ef(i,a,b) for(ll i=a;i<=b;i++)
#define rf(i,a,b) for(ll i=a;i>=b;i--)
#define eb emplace_back
#define mp make_pair
#define INF 0x3f3f3f3f
#define vi vector<pair<ll,ll>>
#define ff first
#define ss second
#define mi map<ll,ll>
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long ull;
#define tez ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define f(i,a,b) for(ll i=a;i<b;i++)
#define ef(i,a,b) for(ll i=a;i<=b;i++)
#define rf(i,a,b) for(ll i=a;i>=b;i--)
#define eb emplace_back
#define mp make_pair
#define INF 0x3f3f3f3f
#define vi vector<pair<ll,ll>>
#define ff first
#define ss second
#define mi map<ll,ll>
void print(string s,int n)
{
    string a="";
    int j=0;
    while(n)
    {
        if(n&1)
        {
            a+=s[j];
        }
        j++;
        n>>=1;
    }
    cout<<a<<endl;
}
void fun(string s)
{
    ll len=s.length();
    ll num=(1<<len);
    f(i,0,num)
    {
        print(s,i);
    }
}
int main()
{
    string s="abc";
    fun(s);
}
